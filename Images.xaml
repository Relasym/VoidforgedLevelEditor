<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LevelEditor">
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Wall}" UriSource="/images/block_cavewall.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Floor}" UriSource="/images/block_filler.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Door}" UriSource="/images/block_door.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Enemy1}" UriSource="/images/block_enemy1.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Enemy2}" UriSource="/images/block_enemy2.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.Player}" UriSource="/images/block_player.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.GameStart}" UriSource="/images/block_gamestart.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.GameEnd}" UriSource="/images/block_gameend.png"/>
    <BitmapImage x:Key="{x:Static local:GameObjectTypes.LevelTransition}" UriSource="/images/block_level_transition.png"/>
</ResourceDictionary>